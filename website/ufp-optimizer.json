{
  "optimizer": {
    "htaccessOptim": {
      "enabled": true,
      "options": {
        "expire": "access plus 1 week",
        "expireHTML": "access plus 1 second",
        "maxageHTML": 1000,
        "maxage": 31536000,
        "outputFile": "/.htaccess"
      }
    }
  },
  "zipOptim": {
    "enabled": true,
    "brotli": {
      "enabled": false
    },
    "zopfli": {
      "enabled": true,
      "options": {}
    },
    "zlib": {
      "enabled": false,
      "options": {}
    }
  }
}
